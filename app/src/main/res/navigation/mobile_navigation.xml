<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/homeFragment"
        android:name="com.yun.mysimplenavi.ui.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_keywordSearchFragment"
            app:destination="@id/keywordSearchFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mapSearchFragment"
            app:destination="@id/mapSearchFragment" />
    </fragment>

    <fragment
        android:id="@+id/keywordSearchFragment"
        android:name="com.yun.mysimplenavi.ui.search.keyword.KeywordSearchFragment"
        android:label="KeywordSearchFragment" >
        <action
            android:id="@+id/action_keywordSearchFragment_to_preViewMapFragment"
            app:destination="@id/preViewMapFragment" />
    </fragment>
    <fragment
        android:id="@+id/findMapFragment"
        android:name="com.yun.mysimplenavi.ui.map.find.FindMapFragment"
        android:label="FindMapFragment" />
    <fragment
        android:id="@+id/preViewMapFragment"
        android:name="com.yun.mysimplenavi.ui.map.preview.PreViewMapFragment"
        android:label="PreViewMapFragment" >
        <action
            android:id="@+id/action_preViewMapFragment_to_findMapFragment"
            app:destination="@id/findMapFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapSearchFragment"
        android:name="com.yun.mysimplenavi.ui.search.map.MapSearchFragment"
        android:label="MapSearchFragment" >
        <action
            android:id="@+id/action_mapSearchFragment_to_preViewMapFragment"
            app:destination="@id/preViewMapFragment" />
    </fragment>


</navigation>